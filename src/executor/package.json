{
  "name": "flooq-executor",
  "version": "0.1.0",
  "main": "./dist/source/Index.js",
  "scripts": {
    "start": "tsc && node ./dist/source/Index.js",
    "dev": "nodemon ./dist/source/Index.js",
    "dev:tsc": "tsc -w",
    "dev:node": "nodemon ./dist/source/Index.js",
    "lint": "eslint -c .eslintrc.js  \"source/**\" \"spec/**\"",
    "lint:fix": "eslint -c .eslintrc.js  \"source/**\" \"spec/**\" --fix",
    "build": "tsc",
    "test": "jest",
    "testOpenHandles": "jest --detectOpenHandles",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-prom-bundle": "^6.4.1",
    "morgan": "^1.10.0",
    "prom-client": "^14.0.1",
    "openid-client": "^5.1.5",
    "typescript": "^4.6.2",
    "vm2": "^3.9.11",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "axios-mock-adapter": "^1.20.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^39.2.9",
    "jest": "^27.5.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "clearMocks": true,
    "setupFiles": [
      "dotenv/config"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "roots": [
      "spec/",
      "source/"
    ]
  }
}
